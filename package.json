    "framer-motion": "6.3.11",
    "next": "12.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@playwright/test": "1.22.2",
    "@testing-library/react": "13.3.0",
    "@types/jest": "28.1.7",
    "@types/node": "17.0.23",
    "@types/react": "18.0.14",
    "eslint": "8.23.1",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
